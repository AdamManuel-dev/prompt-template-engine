{
  "name": "@cursor-prompt/backend",
  "version": "1.0.0",
  "description": "Express.js backend for cursor-prompt web portal with CLI integration",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts --max-warnings 0",
    "lint:fix": "eslint . --ext .ts --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "express",
    "api",
    "cli-integration",
    "template-engine"
  ],
  "author": "Adam Manuel",
  "license": "MIT",
  "dependencies": {
    "@cursor-prompt/shared": "1.0.0",
    "@prisma/client": "^6.15.0",
    "bcrypt": "^6.0.0",
    "chalk": "^4.1.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.4.1",
    "express-validator": "^7.2.0",
    "glob": "^11.0.0",
    "gray-matter": "^4.0.3",
    "helmet": "^8.0.0",
    "joi": "^18.0.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cache": "^5.1.2",
    "passport": "^0.7.0",
    "passport-github2": "^0.1.12",
    "passport-google-oauth20": "^2.0.0",
    "prisma": "^6.15.0",
    "uuid": "^11.0.4",
    "ws": "^8.18.3"
